@incollection{ ISI:000418007600108,
Author = {Cooper, Antony K. and Coetzee, Serena and Kourie, Derrick G.},
Book-Group-Author = {{Informat Resources Management Assoc}},
Title = {{An Assessment of Several Taxonomies of Volunteered Geographic
   Information}},
Booktitle = {{GEOGRAPHIC INFORMATION SYSTEMS: CONCEPTS, METHODOLOGIES, TOOLS, AND
   APPLICATIONS, VOL 1}},
Year = {{2013}},
Pages = {{1794-1808}},
Abstract = {{User-Generated Content (UGC) in general, and Volunteered Geographical
   Information (VGI) in particular, are becoming more important as sources
   for official data bases, such as those used in national Spatial Data
   Infrastructures (SDIs). Discovering and assessing VGI as suitable
   geospatial resources for one's purposes is hence becoming more
   important, but can be difficult. One way of assessing VGI resources is
   by classifying them into different types of resources, i.e. a taxonomy
   of resources. The question is whether such taxonomies can accurately
   identify suitable VGI resources. We assess five taxonomies both
   subjectively and using formal concept analysis to determine their
   discrimination adequacy, that is, how well the taxonomies discriminate
   between repositories containing UGC in general, or VGI in particular.}},
DOI = {{10.4018/978-1-4666-2038-4.ch107}},
ISBN = {{978-1-4666-2039-1; 978-1-4666-2038-4}},
ORCID-Numbers = {{Cooper, Antony/0000-0001-9411-2094}},
