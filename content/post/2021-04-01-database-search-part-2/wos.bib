@article{ ISI:000439590700003,
Author = {Minner, Jennifer and Holleran, Michael and Roberts, Andrea and Conrad,
   Joshua},
Title = {{Capturing Volunteered Historical Information: Lessons from Development
   of a Local Government Crowdsourcing Tool}},
