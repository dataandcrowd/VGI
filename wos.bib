@article{ ISI:000414650600064,
Author = {Zhao, Yuanyuan and Feng, Duole and Yu, Le and See, Linda and Fritz,
   Steffen and Perger, Christoph and Gong, Peng},
Title = {{Assessing and Improving the Reliability of Volunteered Land Cover
   Reference Data}},
