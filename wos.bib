@article{ ISI:000416793400230,
Author = {Cao, Xiaoshu and Liang, Feiwen and Chen, Huiling and Liu, Yongwei},
Title = {{Circuity Characteristics of Urban Travel Based on GPS Data: A Case Study
   of Guangzhou}},
