Language = {{English}},
Affiliation = {{Johnson, PA (Corresponding Author), Univ Waterloo, Dept Geog \& Environm Management, Waterloo, ON, Canada.
   Johnson, Peter A., Univ Waterloo, Dept Geog \& Environm Management, Waterloo, ON, Canada.}},
DOI = {{10.1080/15230406.2016.1176536}},
