@article{ ISI:000381499600009,
Author = {Gomez-Barron, Jose-Pablo and Manso-Callejo, Miguel-Angel and Alcarria,
   Ramon and Iturrioz, Teresa},
Title = {{Volunteered Geographic Information System Design: Project and
   Participation Guidelines}},
