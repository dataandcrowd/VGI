@article{ ISI:000464345500001,
Author = {Hughes, Lloyd Haydn and Streicher, Simon and Chuprikova, Ekaterina and
   du Preez, Johan},
Title = {{A Cluster Graph Approach to Land Cover Classification Boosting}},
