@article{ ISI:000419538500042,
Author = {Mauro, G. and Lughi, V.},
Title = {{Mapping land use impact of photovoltaic farms via crowdsourcing in the
   Province of Lecce (Southeastern Italy)}},
Journal = {{SOLAR ENERGY}},
