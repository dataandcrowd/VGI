@article{ ISI:000585771100001,
Author = {Klonner, Carolin and Uson, Tomas J. and Aeschbach, Nicole and Hoefle,
   Bernhard},
Title = {{Participatory Mapping and Visualization of Local Knowledge: An Example
   from Eberbach, Germany}},
Journal = {{INTERNATIONAL JOURNAL OF DISASTER RISK SCIENCE}},
