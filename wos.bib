@article{ ISI:000594547800001,
Author = {Sebastian-Lopez, Maria and de Miguel Gonzalez, Rafael},
Title = {{Mobile Learning for Sustainable Development and Environmental Teacher
   Education}},
Journal = {{SUSTAINABILITY}},
