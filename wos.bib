@article{ ISI:000569750200001,
Author = {Hologa, Rafael and Riach, Nils},
Title = {{Approaching Bike Hazards via Crowdsourcing of Volunteered Geographic
   Information}},
Journal = {{SUSTAINABILITY}},
Year = {{2020}},
Volume = {{12}},
Number = {{17}},
Month = {{SEP}},
Abstract = {{Information on individual hazard perception while cycling and the
   associated feeling of safety are key aspects to foster sustainable urban
   cycling mobility. Although cyclist's perceptions must also be critically
   reviewed, such crowdsourced Volunteered Geographic Information (VGI)
   provides wide-ranging insights on diverse hazard categories in cycling.
   In this case study in the city of Freiburg, Germany, hazard perceptions,
   information about lane types, and the underlying routes were
   crowdsourced via an open source smartphone application by a small group
   with the aim of providing cyclists with effective solutions. By dealing
   with levels of reliability, we show that even a small group of
   laypersons can generate an extensive and valuable set of VGI consisting
   of comprehensive hazard categories. We demonstrate that (1) certain
   hazards are interlinked to specific lane types, and (2) the individual
   hazard perceptions and objective parameters, i.e., accident data, are
   often congruent spatially; consequently, (3) dangerous hot spots can be
   derived. By considering cyclists' needs, this approach outlines how a
   people-based perspective can supplement regional planning on the local
   scale.}},
